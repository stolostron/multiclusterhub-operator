{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "addLabels": [
        "ok-to-test"
    ],
    "baseBranchPatterns": [
        "/(^main$)|(^release-2\\.(11|1[2-9]|[2-9][0-9]+)$)/"
    ],
    "rebaseWhen": "behind-base-branch",
    "recreateWhen": "never",
    "schedule": [
        "before 8am on tuesday and thursday"
    ],
    "timezone": "America/New_York"
}
